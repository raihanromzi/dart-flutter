  ...
}

void main(List<String> arguments){
  say("Romzi", "Hello", "Zaidan");
  say("Romzi", "Hello", "Zaidan", "Line");
  
//Default Parameter Value *Use = --> memberikan nilai default pada optional parameter. klo gapake NULL.
